git push https://gitlab.com/QGAB-Team/qq-bot-doc.git
git push https://jihulab.com/QGAB-Team/qq-bot-doc.git
git push https://gitee.com/QGAB-Team/qq-bot-doc.git
cd ../
rm -rf qq-bot-doc